@article{Belton:1983,
	Abstract = {This article highlights the ambiguity inherent in the term 'weight'. The examples have demonstrated an apparent anomaly in Saaty's method if one interprets weight in the sense of multi-attribute utility methods. This can easily be overcome by a modified normalisation procedure, together with a redefined question to make the pairwise comparisons of attributes. In general, real life decision problems are not of the carefully constructed nature of our examples. Thus. for the analyst to be able to interpret the information provided by the decision-maker in a sensible way, it is important that the pairwise comparison questions are more specific than those advocated by Saaty at present. The decision-maker should be clear about, and the analyst aware of. the factors taken into account in specifying weights. The anal)st can then adopt an appropriate method to process the information. This need may not always be the multi-attribute utility approach, or the Saaty approach, or any other specific approach, but that with which the decision-maker feels at ease.

},
	Annote = {Rank reversal in AHP when a new alternative is added. Happens in some cases, due to normalization method used by Saaty. Belton and Gear propose a different normalization method.},
	Author = {Valerie Belton and Tony Gear},
	Date-Added = {2015-03-12 15:10:35 +0000},
	Date-Modified = {2015-10-22 17:19:05 +0000},
	Doi = {10.1016/0305-0483(83)90047-6},
	Journal = {Omega},
	Keywords = {AHP},
	Number = {3},
	Pages = {228 - 230},
	Title = {On a short-coming of Saaty's method of analytic hierarchies},
	Volume = {11},
	Year = {1983},
	Bdsk-File-1 = {}}

@article{Belton:1986,
	Abstract = {The paper reviews the applicability of approaches to multiple criteria decision making to aiding in the selection of a preferred option from a short-list of alternatives in the light of a wealth of information about those alternatives. It is concluded that Saaty's Analytic Hierarchy Process and a Simple Multi-Attributed Value Function are the approaches best suited to this problem and the most widely used in practice. The paper goes on to compare these approaches in detail from both a theoretical and a practical standpoint. The underlying assumptions of each are examined and considered in the light of practical decision aiding. The practical implications of the use of each approach are discussed. Reference is made to experimental work carried out by the author which has examined both practical and theoretical aspects of the use of each approach in isolation and in comparative studies. The paper will be of relevance to anyone facing a decision involving a finite number of alternatives and multiple criteria and considering the use of a formal approach. },
	Annote = {Summarizes criticisms of AHP. 

Compares AHP pairwise comparisons to using MultiAttribue Value scoring on an interval scale. Attributes systematic discrepency/distortion to difference between semantic and numeric scales.

Summarizes strenghts and weaknesses of both approaches.},
	Author = {Valerie Belton},
	Date-Added = {2015-03-12 15:06:51 +0000},
	Date-Modified = {2015-10-22 17:19:05 +0000},
	Doi = {10.1016/0377-2217(86)90155-4},
	Journal = {European Journal of Operational Research},
	Keywords = {AHP},
	Number = {1},
	Pages = {7 - 21},
	Title = {A comparison of the analytic hierarchy process and a simple multi-attribute value function},
	Volume = {26},
	Year = {1986},
