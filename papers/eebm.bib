	Keywords = {Approximation methods;Biological system modeling;Computational modeling;Optimization;Sociology;Software;Standards;Active Learning;Multi-objective optimization;Search based software engineering},
	Number = {99},
	Title = {GALE: Geometric Active Learning for Search-Based Software Engineering},
	Url = {http://ieeexplore.ieee.org/xpls/abs_all.jsp?arnumber=7105950},
	Year = {2015},
